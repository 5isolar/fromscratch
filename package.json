{
  "name": "FromScratch",
  "version": "1.0.1",
  "description": "simple autosaving scratchpad",
  "main": "main.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive",
    "test-watch": "npm test -- --watch",
    "lint": "eslint .",
    "hot-server": "node server.js",
    "build": "NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "electron .",
    "start-hot": "HOT=1 NODE_ENV=development electron .",
    "package": "NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Kilian Valkhof",
    "email": "kilian@kilianvalkhof.com",
    "url": "https://kilianvalkhof.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kilian/fromscratch/issues"
  },
  "keywords": [
    "electron",
    "react",
    "webpack",
    "scratchpad",
    "coidemirror"
  ],
  "homepage": "https://github.com/kilian/fromscratch",
  "devDependencies": {
    "asar": "^0.8.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "css-loader": "^0.23.0",
    "del": "^2.0.1",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.3",
    "electron-rebuild": "^1.0.0",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "minimist": "^1.2.0",
    "mocha": "^2.3.0",
    "node-libs-browser": ">= 0.4.0 <=0.6.0",
    "node-sass": "^3.4.1",
    "proxyquire": "^1.7.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "sass-loader": "^3.1.1",
    "sinon": "^1.16.1",
    "style-loader": "^0.13.0",
    "url-loader": "0.5.6",
    "webpack": "^1.12.3",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "electron-debug": "^0.5.1",
    "keymirror": "^0.1.1",
    "node-localstorage": "^1.1.2",
    "object-assign": "^4.0.1",
    "react": "^0.14.2",
    "react-codemirror": "^0.2.0",
    "react-dom": "^0.14.2"
  }
}
